{
  "name": "quizmaster-client-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "@microsoft/signalr": "^3.1.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.39",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.19.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "jest-junit-reporter": "^1.1.0",
    "material-table": "^1.57.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "typeface-roboto": "0.0.75",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start-web-app": "react-scripts start",
    "start-api": "dotnet run -p ..\\QuizMaster.Api\\QuizMaster.Api.csproj",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "npm-run-all --parallel start-api start-web-app",
    "ci-test": "react-scripts test --coverage --coverageReporters=cobertura --watchAll=false --testResultsProcessor ./node_modules/jest-junit-reporter",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "lint": "eslint \"./src/**/*.{js,ts,tsx}\" --quiet --fix",
    "ci-lint": "eslint \"./src/**/*.{js,ts,tsx}\" --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.5",
    "husky": "^4.2.5",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.2.7",
    "npm-run-all": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
